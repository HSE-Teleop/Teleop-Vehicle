{
  "Vehicle": {
    "children": {
      "Chassis": {
        "children": {
          "FrontCamera": {
            "datatype": "boolean",
            "description": "Check if the frontcamera exists.",
            "type": "sensor"
          },
          "RearCamera": {
            "datatype": "boolean",
            "description": "Check if the rearcamera exists.",
            "type": "sensor"
          }
        },
        "comment": "Is currently used for information data but holds in original tree the PedalPosition. Will be considered using this branch for acceleration when its clear how the subsystem operates.",
        "description": "All data concerning steering, suspension, wheels, and brakes.",
        "type": "branch"
      },
      "Identification": {
        "children": {
          "VIN": {
            "datatype": "string",
            "description": "17-character Vehicle Identification Number (VIN) as defined by ISO 3779.",
            "pattern": "^([0-9A-HJ-NPR-Z]{3})([0-9A-HJ-NPR-Z]{6})([0-9A-HJ-NPR-Z]{4}[0-9]{4})$",
            "type": "attribute"
          }
        },
        "description": "Identification data for the vehicle.",
        "type": "branch"
      },
      "PowerTrain": {
        "children": {
          "Transmission": {
            "children": {
              "CurrentGear": {
                "datatype": "int8",
                "description": "The current gear. -1 = Reverse, 0 = Neutral, 1 = Forward.",
                "max": 1,
                "min": -1,
                "type": "sensor"
              }
            },
            "description": "Transmission-specific data, stopping at the drive shafts.",
            "type": "branch"
          }
        },
        "description": "Powertrain data for battery management, etc.",
        "type": "branch"
      },
      "Speed": {
        "datatype": "float",
        "dbc2vss": {
          "interval_ms": 10,
          "signal": "VelocityFb",
          "transform": {
            "math": "max(-1,min(1,(x-90)/15))"
          }
        },
        "description": "Vehicle speed.",
        "type": "sensor",
        "unit": "km/h"
      },
      "Teleop": {
        "children": {
          "ControlCounter": {
            "datatype": "uint8",
            "description": "Counter",
            "type": "actuator",
            "vss2dbc": {
              "signal": "ControlCounter"
            }
          },
          "ControlTimestampMs": {
            "datatype": "uint32",
            "description": "Timestamp in ms",
            "type": "actuator",
            "unit": "ms",
            "vss2dbc": {
              "signal": "ControlTimestamp_ms"
            }
          },
          "EnginePower": {
            "datatype": "float",
            "description": "Percentual power of the engine. <0 = Engine works against its current speed, 0 = Neutral state of the engine, >0 = Engine runs with a percentage of its power.",
            "max": 1000,
            "min": -1000,
            "type": "actuator",
            "unit": "percent",
            "vss2dbc": {
              "signal": "AccelerationCmd",
              "transform": {
                "math": "round((x*15)+90)"
              }
            }
          },
          "Gear": {
            "datatype": "int8",
            "description": "The current gear. -1 = Reverse, 0 = Neutral, 1 = Forward.",
            "max": 1,
            "min": -1,
            "type": "attribute"
          },
          "HeartBeat": {
            "datatype": "int8",
            "description": "Keep-alive signal so that the vehicle knows that there is still a connection.",
            "type": "attribute"
          },
          "SteeringAngle": {
            "datatype": "int16",
            "description": "Represents the current input angle of the steering system, typically corresponds to angle applied by driver on the steering-wheel. Positive for angle in counterclockwise direction (based on ISO8855).",
            "type": "actuator",
            "unit": "degrees",
            "vss2dbc": {
              "signal": "SteeringCmd"
            },
            "dbc2vss": {
              "signal": "SteeringFb"
            }
          }
        },
        "description": "Holds commands for teleoperation data.",
        "type": "branch"
      }
    },
    "description": "High-level vehicle data.",
    "type": "branch"
  }
}